{
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "indent": [1, 4],
        "linebreak-style": [2, "unix"],
        "eol-last": [2, "always"],
        "quotes": [1, "single"],
        "jsx-quotes": [1, "single"],
        "comma-dangle": [1, "always-multiline"]
    }
}
